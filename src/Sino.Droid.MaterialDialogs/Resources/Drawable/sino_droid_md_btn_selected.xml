      android:shape="rectangle">
    <corners android:radius="@dimen/sino_droid_md_action_corner_radius" />
    <solid android:color="@color/sino_droid_md_btn_selected" />
    <padding
        android:bottom="@dimen/sino_droid_md_button_padding_vertical"
        android:left="@dimen/sino_droid_md_button_padding_horizontal"
        android:right="@dimen/sino_droid_md_button_padding_horizontal"
        android:top="@dimen/sino_droid_md_button_padding_vertical" />
  </shape>
</inset>