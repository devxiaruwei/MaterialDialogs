<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetBottom="@dimen/sino_droid_md_button_inset_vertical"
    android:insetLeft="@dimen/sino_droid_md_button_inset_horizontal"
    android:insetRight="@dimen/sino_droid_md_button_inset_horizontal"
    android:insetTop="@dimen/sino_droid_md_button_inset_vertical">
